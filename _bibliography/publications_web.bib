@article{Kaneda:17,
author = {Fumihiro Kaneda and Feihu Xu and Joseph Chapman and Paul G. Kwiat},
journal = {Optica},
keywords = {Quantum optics; Photon statistics; Quantum communications ; Quantum information and processing ; Nonlinear parametric processes; Quantum communications; Quantum information processing; Quantum key distribution; Quantum memories; Quantum memory},
number = {9},
pages = {1034--1037},
publisher = {OSA},
title = {Quantum-memory-assisted multi-photon generation for efficient quantum information processing},
volume = {4},
month = {Sep},
year = {2017},
url = {http://www.osapublishing.org/optica/abstract.cfm?URI=optica-4-9-1034},
doi = {10.1364/OPTICA.4.001034},
abstract = {Efficient preparation of large, but definite, numbers of photons is of great importance for scaling up and speeding up photonic quantum information processing. Typical single-photon generation techniques based on nonlinear parametric processes face challenges of probabilistic generation. Here we demonstrate efficient synchronization of photons from multiple nonlinear parametric heralded single-photon sources, using quantum memories. Our low-loss optical memories greatly enhance (\&\#x223C;30\&\#x00D7;) the generation rate of coincidence photons from two independent sources, while maintaining high indistinguishability (95.7\&\#x00B1;1.4\%) of the synchronized photons. As an application, we perform the first demonstration of parametric-source-based measurement-device-independent quantum key distribution. The synchronization technique demonstrated here paves the way toward efficient quantum communication and larger-scale optical quantum computing.},
}

@article{Hill:17,
author = {Alexander D. Hill and David Hervas and Joseph Nash and Martin Graham and Alexander Burgers and Uttam Paudel and Duncan Steel and Christian Schneider and Martin Kamp and Sven H\"{o}fling and Jin Wang and Jiahe Lin and Wanying Zhao and Paul G. Kwiat},
journal = {Opt. Express},
keywords = {Quantum optics; Active or adaptive optics; Deformable mirrors; Genetic algorithms; Parabolic mirrors; Quantum dots; Quantum information processing; Wavefront aberrations},
number = {16},
pages = {18629--18642},
publisher = {OSA},
title = {Optimizing single-mode collection from pointlike sources of single photons with adaptive optics},
volume = {25},
month = {Aug},
year = {2017},
url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-25-16-18629},
doi = {10.1364/OE.25.018629},
abstract = {The collection efficiency of light from a point-like emitter may be extremely poor due to aberrations induced by collection optics and the emission distribution of the source. Analyzing the aberrant wavefront (e.g., with a Shack-Hartmann sensor) and correcting accordingly can be infeasible on the single-photon level. We present a technique that uses a genetic algorithm to control a deformable mirror for correcting wavefront aberrations in single-photon signals from point emitters. We apply our technique to both a simulated point source and a real InAs quantum dot, achieving coupling increases of up to 50\% and automatic reduction of system drift.},
}

